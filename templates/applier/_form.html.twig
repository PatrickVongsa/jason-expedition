{{ form_start(form) }}
    <div class="row">
        <h4 class="mb-2">Etat civil</h4>
        <div class="col">
            {{ form_row(form.firstname) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.lastname) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(form.age) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.gender) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.height) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(form.origin) }}
        </div>
        <div class=" col"></div>
    </div>
        <h4 class="mt-3 mb-2">Compétences</h4>
    <div class="row">
        <div class="col">
            {{ form_row(form.profession) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.warriorSkill) }}
        </div>
    </div>
    <div class="row">
        <div class="col">
            {{ form_row(form.weapon) }}
        </div>
        <div class="col" id="some-custom-id">
            {{ form_row(form.navigationSkill) }}
        </div>
    </div>

    <button class="btn btn-primary my-3">{{ button_label|default('Créer') }}</button>
{{ form_end(form) }}
